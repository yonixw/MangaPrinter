<Window x:Class="MangaPrinter.WpfGUI.Dialogs.dlgChooseCutoffRatio"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MangaPrinter.WpfGUI.Dialogs"
        xmlns:d3="clr-namespace:InteractiveDataDisplay.WPF;assembly=InteractiveDataDisplay.WPF"
        mc:Ignorable="d"
        Title="Choose cutoff ratio for double-page" Height="450" Width="800"  WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <d3:Chart BottomTitle="Ratio (Page Width/Height)" LeftTitle="Count">
            <Grid>
                <d3:LineGraph x:Name="gRatio" Description="Ratio Distribution" Stroke="Blue" StrokeThickness="3"/>
                <d3:LineGraph x:Name="gCutoff" Description="Ratio Distribution" Stroke="Red" StrokeThickness="3"/>
            </Grid>
        </d3:Chart>
    </Grid>
</Window>
